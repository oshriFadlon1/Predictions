<?xml version="1.0" encoding="UTF-8"?>
<PRD-world xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="predictions-v1.xsd">
    <PRD-evironment>
        <PRD-env-property type="decimal">
            <PRD-name>cigarets-critical</PRD-name>
            <PRD-range from="10" to="100"/>
        </PRD-env-property>
        <PRD-env-property type="decimal">
            <PRD-name>cigarets-increase-non-smoker</PRD-name>
            <PRD-range from="0" to="10"/>
        </PRD-env-property>
        <PRD-env-property type="decimal">
            <PRD-name>cigarets-increase-already-smoker</PRD-name>
            <PRD-range from="10" to="100"/>
        </PRD-env-property>
    </PRD-evironment>
    <PRD-entities>
        <PRD-entity name="Smoker">
            <PRD-population>1</PRD-population>
            <PRD-properties>
                <PRD-property type="decimal">
                    <PRD-name>lung-cancer-progress</PRD-name>
                    <PRD-range to="100" from="0"/>
                    <PRD-value random-initialize="false" init="0"/>
                </PRD-property>
                <PRD-property type="decimal">
                    <PRD-name>age</PRD-name>
                    <PRD-range to="50" from="15"/>
                    <PRD-value random-initialize="false" init="15"/>
                </PRD-property>
                <PRD-property type="decimal">
                    <PRD-name>cigarets-per-month</PRD-name>
                    <PRD-range to="500" from="0"/>
                    <PRD-value random-initialize="false" init="450"/>
                </PRD-property>
            </PRD-properties>
        </PRD-entity>
    </PRD-entities>
    <PRD-rules>
        <PRD-rule name="aging">
            <PRD-actions>
                <PRD-action entity="Smoker" type="increase" property="age" by="1"/>
            </PRD-actions>
            <PRD-activation ticks="12"/>
        </PRD-rule>
        <PRD-rule name="smokeMoreAnd">
            <PRD-actions>
                <PRD-action entity="Smoker" type="increase" property="cigarets-per-month" by="2"/>
                <PRD-action entity="Smoker" type="decrease" property="cigarets-per-month" by="3"/>
            </PRD-actions>
            <PRD-activation ticks="3"/>
        </PRD-rule>
    </PRD-rules>
    <PRD-termination>
        <PRD-by-ticks count="25"/>
        <PRD-by-second count="10"/>
    </PRD-termination>
</PRD-world>
